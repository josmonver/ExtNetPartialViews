@using Ext.Net.MVC;

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Partials Views demo</title>
    @Styles.Render("~/Content/css")

</head>
<body>
    @{
        var X = Html.X();
    }

    @Html.X().ResourceManager()
    
    <p>This is _Layout</p>

    @Html.Partial("_Menu")
    @RenderBody()

    <footer>
        <p style="padding-left:5px">This is _Layout</p>
    </footer>

    @Scripts.Render("~/bundles/js")
    @RenderSection("scripts", required: false)
</body>
</html>
